{
  "compileOnSave": false,
  "buildOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "data": ["data"],
      "components": ["src/components"],
      "styles": ["src/styles"]
    },
    "types": [
      "node"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./definitions"
    ]
  }
}
